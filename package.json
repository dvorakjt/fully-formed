{
  "name": "fully-formed",
  "version": "0.0.3",
  "description": "Powerful, type-safe, style-agnostic forms for React.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "test": "node src-exists.cjs && vitest",
    "lint": "node src-exists.cjs && eslint --ext .js,.ts,.tsx .",
    "format": "node src-exists.cjs && prettier --write \"**/*.+(js|ts|tsx|json)\"",
    "build": "node src-exists.cjs && tsup"
  },
  "files": [
    "/dist",
    "src-exists.cjs"
  ],
  "keywords": [
    "react",
    "form"
  ],
  "author": "Joseph Dvorak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvorakjt/fully-formed.git"
  },
  "bugs": {
    "url": "https://github.com/dvorakjt/fully-formed/issues"
  },
  "homepage": "https://github.com/dvorakjt/fully-formed#readme",
  "devDependencies": {
    "@testing-library/react": "^15.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "dependencies": {
    "rxjs": "^7.8.1"
  },
  "peerDependencies": {
    "react": ">=18",
    "typescript": ">=5"
  }
}
